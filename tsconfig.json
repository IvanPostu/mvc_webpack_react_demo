{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "esModuleInterop": true, 
    "lib": ["es6", "dom"],
    "moduleResolution": "node",
    "strict": true,                           
    "module": "commonjs",
    "target": "ES3",
    "jsx": "react",
    "allowJs": true,
    "skipLibCheck": true,                     
    "forceConsistentCasingInFileNames": true,  
    "baseUrl": ".",
  }
}
